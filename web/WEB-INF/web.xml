<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CartServlet</servlet-name>
        <servlet-class>controller.CartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Del_Cart</servlet-name>
        <servlet-class>controller.Del_Cart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login_adServerlet</servlet-name>
        <servlet-class>controller.login_adServerlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginad</servlet-name>
        <servlet-class>controller.loginad</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login_ad</servlet-name>
        <servlet-class>controller.Login_ad</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CartServlet</servlet-name>
        <url-pattern>/CartServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Del_Cart</servlet-name>
        <url-pattern>/Del_Cart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login_adServerlet</servlet-name>
        <url-pattern>/login_adServerlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginad</servlet-name>
        <url-pattern>/loginad</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login_ad</servlet-name>
        <url-pattern>/Login_ad</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30 
        </session-timeout>
        <cookie-config>
        </cookie-config>
    </session-config>
</web-app>
